{
    "routes": [
        {
            "route": "/scss/*"
        },
        {
            "route": "/js/*"
        },
        {
            "route": "/favicons/*"
        },
        {
            "route": "/webfonts/*"
        },
        {
            "route": "/favicon/*"
        },
        {
            "route": "/.auth/login/github"
        },
        {
            "route": "/.auth/login/google"
        },
        {
            "route": "/.auth/login/aad"
        },
        {
            "route": "/.auth/invitations/accept"
        },
        {
            "route": "/.auth/logout"
        },
        {
            "route": "/.auth/me"
        },
        {
            "route": "/login/"
        },
        {
            "route": "/*",
            "allowedRoles": ["verified"]
        }
    ],
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/login/"
        }
    }
}